!MarsRover methodsFor: 'commands' stamp: 'AB 10/29/2022 17:02:09' prior: 50554177!
executeCommand: aCommand
	(aCommand = $f) ifTrue: [^self forward].
	(aCommand = $b) ifTrue: [^(position := (cardinalPoint backward: position))].
	(aCommand = $l) ifTrue: [^	self rotateToLeft].
